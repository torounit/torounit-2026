{
	"$schema": "https://schemas.wp.org/wp/6.9/theme.json",
	"version": 3,
	"settings": {
		"appearanceTools": true,
		"color": {
			"defaultDuotone": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"palette": [
				{
					"color": "#FFF",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "oklch(98.5% 0.002 247.839)",
					"name": "Smoke",
					"slug": "smoke"
				},
				{
					"color": "oklch(20.8% 0.042 265.755)",
					"name": "Contrast",
					"slug": "contrast"
				}
			]
		},
		"layout": {
			"contentSize": "856px",
			"wideSize": "1384px"
		},
		"spacing": {
			"defaultSpacingSizes": false,
			"spacingSizes": [
				{
					"name": "Tiny",
					"size": "calc( var(--wp--preset--font-size--medium) / 2 )",
					"slug": "20"
				},
				{
					"name": "X-Small",
					"size": "calc( var(--wp--preset--font-size--medium) / 2 * 2 )",
					"slug": "30"
				},
				{
					"name": "Small",
					"size": "calc( var(--wp--preset--font-size--medium) / 2 * 3 )",
					"slug": "40"
				},
				{
					"name": "Medium",
					"size": "calc( var(--wp--preset--font-size--medium) / 2 * 5 )",
					"slug": "50"
				},
				{
					"name": "Large",
					"size": "calc( var(--wp--preset--font-size--medium) / 2 * 8 )",
					"slug": "60"
				},
				{
					"name": "X-Large",
					"size": "calc( var(--wp--preset--font-size--medium) / 2 * 13 )",
					"slug": "70"
				},
				{
					"name": "XX-Large",
					"size": "calc( var(--wp--preset--font-size--medium) / 2 * 21 )",
					"slug": "80"
				}
			],
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"defaultFontSizes": false,
			"fontFamilies": [
				{
					"fontFamily": "'Zen Kaku Gothic New', sans-serif",
					"name": "Zen Kaku Gothic New",
					"slug": "zen-kaku-gothic-new"
				},
				{
					"fontFamily": "'M Plus 1 Code', monospace",
					"name": "M Plus 1 Code",
					"slug": "m-plus-1-code"
				}
			],
			"fontSizes": [
				{
					"name": "small",
					"size": "calc( var(--wp--preset--font-size--medium) * 8 / 10 )",
					"slug": "small"
				},
				{
					"name": "Medium",
					"size": "1rem",
					"slug": "medium"
				},
				{
					"name": "large",
					"size": "calc( var(--wp--preset--font-size--medium) * 8 / 7 )",
					"slug": "large"
				},
				{
					"name": "Extra Large",
					"size": "calc( var(--wp--preset--font-size--medium) * 8 / 5 )",
					"slug": "x-large"
				},
				{
					"name": "Extra Extra Large",
					"size": "calc( var(--wp--preset--font-size--medium) * 8 / 3 )",
					"slug": "xx-large"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/button": {
				"variations": {
					"outline": {
						"spacing": {
							"padding": {
								"bottom": "calc( var(--wp--preset--spacing--20) - 2px)",
								"left": "calc( var(--wp--preset--spacing--40) - 2px)",
								"right": "calc( var(--wp--preset--spacing--40) - 2px)",
								"top": "calc( var(--wp--preset--spacing--20) - 2px)"
							}
						}
					}
				}
			},
			"core/code": {
				"color": {
					"background": "var(--wp--preset--color--smoke)"
				},
				"css": "background-image: linear-gradient(0deg, transparent 0, color-mix(in srgb, currentColor 20%, transparent) 1px, transparent 1px);background-size: auto 1lh",
				"spacing": {
					"padding": {
						"left": "var(--wp--preset--spacing--20)",
						"right": "var(--wp--preset--spacing--20)"
					}
				},
				"typography": {
					"lineHeight": "2",
					"fontWeight": "200",
					"fontFamily": "var(--wp--preset--font-family--m-plus-1-code)"
				}
			},
			"core/preformatted": {
				"color": {
					"background": "var(--wp--preset--color--smoke)"
				},
				"spacing": {
					"padding": {
						"top": "var(--wp--preset--spacing--30)",
						"bottom": "var(--wp--preset--spacing--30)",
						"left": "var(--wp--preset--spacing--30)",
						"right": "var(--wp--preset--spacing--30)"
					}
				},
				"typography": {
					"fontWeight": "200",
					"fontFamily": "var(--wp--preset--font-family--m-plus-1-code)"
				}
			},
			"core/navigation": {
				"css": "--wp--style--block-gap: var(--wp--preset--spacing--40)",
				"elements": {
					"link": {
						"spacing": {
							"padding": {
								"bottom": "0px !important",
								"top": "0px !important"
							}
						},
						"typography": {
							"textDecoration": "underline"
						}
					}
				},
				"spacing": {
					"blockGap": "var(--wp--preset--spacing--40)"
				},
				"typography": {
					"lineHeight": "2"
				}
			},
			"core/navigation-submenu": {
				"spacing": {
					"blockGap": "var(--wp--preset--spacing--30)",
					"padding": {
						"bottom": "0px !important",
						"left": "var(--wp--preset--spacing--30) !important",
						"right": "var(--wp--preset--spacing--30) !important",
						"top": "0px !important"
					}
				}
			},
			"core/post-title": {
				"elements": {
					"link": {
						"color": {
							"text": "inherit"
						},
						"typography": {
							"textDecoration": "underline"
						}
					}
				}
			},
			"core/separator": {
				"color": {
					"background": "var(--wp--preset--color--contrast)"
				}
			},
			"core/site-title": {
				"elements": {
					"link": {
						"color": {
							"text": "inherit"
						},
						"typography": {
							"textDecoration": "underline"
						}
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--base)",
			"text": "var(--wp--preset--color--contrast)"
		},
		"css": "[class*='font-size'] { line-height: round(up, calc( 1em * 1.75 ), 4px ); }",
		"elements": {
			"button": {
				"border": {
					"radius": "0.25rem"
				},
				"color": {
					"background": "var(--wp--preset--color--contrast)",
					"text": "var(--wp--preset--color--base)"
				},
				"spacing": {
					"padding": {
						"bottom": "var(--wp--preset--spacing--20)",
						"left": "var(--wp--preset--spacing--40)",
						"right": "var(--wp--preset--spacing--40)",
						"top": "var(--wp--preset--spacing--20)"
					}
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xx-large)",
					"fontWeight": "400"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)",
					"fontWeight": "400"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontWeight": "400"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "400"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontWeight": "700"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontWeight": "400"
				}
			},
			"heading": {
				"css": "line-height: round(up, calc( 1em * 1.75 ), 4px );"
			},
			"link": {
				"color": {
					"text": "inherit"
				},
				"css": "text-underline-offset: 0.125em",
				"typography": {
					"textDecoration": "underline"
				}
			},
			"select": {
				"border": {
					"color": "var(--wp--preset--color--contrast)",
					"radius": "0.25rem",
					"width": "2px"
				},
				"css": "appearance: base-select; &::picker(select) {appearance: base-select;} &::picker-icon { font-family: 'Material Symbols Outlined'; appearance: base-select;  content: 'arrow_drop_down';}",
				"spacing": {
					"padding": {
						"bottom": "var(--wp--preset--spacing--20)",
						"left": "var(--wp--preset--spacing--30)",
						"right": "var(--wp--preset--spacing--30)",
						"top": "var(--wp--preset--spacing--20)"
					}
				}
			},
			"textInput": {
				"border": {
					"color": "var(--wp--preset--color--contrast)",
					"radius": "0.25rem",
					"width": "2px"
				}
			}
		},
		"spacing": {
			"blockGap": "var(--wp--preset--spacing--40)",
			"padding": {
				"left": "var(--wp--preset--spacing--40)",
				"right": "var(--wp--preset--spacing--40)"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--zen-kaku-gothic-new)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"fontWeight": "400",
			"lineHeight": "1.75"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	]
}